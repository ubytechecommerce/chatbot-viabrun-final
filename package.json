{
  "name": "chatbot-viabrun-final",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && (cd server && tsc)",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cd server && tsc && node dist/index.js",
    "server:dev": "cd server && tsc --watch --preserveWatchOutput",
    "test-server": "node simple-test-server.cjs",
    "dev:all": "concurrently \"npm run dev\" \"npm run test-server\"",
    "start": "npm run build && npm run server"
  },
  "dependencies": {
    "@google/generative-ai": "^0.14.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
